<?xml version="1.0" encoding="UTF-8"?>
<diagramClass:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:diagramClass="http://www.xtext.org/smokingtext/diagramclass/DiagramClass">
  <greetings name="default">
    <classType xsi:type="diagramClass:Enumeration" classname="Marque"/>
    <classType xsi:type="diagramClass:Class" classname="Roue">
      <body xsi:type="diagramClass:Attribute" attributename="taille" type="integer"/>
      <body xsi:type="diagramClass:Method" name="tourne" returnType="boolean">
        <param name="angle" type="integer"/>
      </body>
    </classType>
    <classType xsi:type="diagramClass:Class" classname="Moteur">
      <body xsi:type="diagramClass:Attribute" attributename="puissance" type="integer"/>
    </classType>
    <classType xsi:type="diagramClass:Interface" interfacename="Vehicule">
      <body name="avance"/>
    </classType>
    <classType xsi:type="diagramClass:Abstract" classname="VehiculeMotorise" extension="Vehicule">
      <body xsi:type="diagramClass:Attribute" attributename="marque" type="Marque"/>
      <body xsi:type="diagramClass:Attribute" attributename="moteur" type="Moteur"/>
    </classType>
    <classType xsi:type="diagramClass:Class" classname="Voiture" extension="VehiculeMotorise">
      <body xsi:type="diagramClass:Attribute" attributename="roues" type="Roue"/>
      <body xsi:type="diagramClass:Attribute" attributename="nbPorte" type="integer"/>
      <body xsi:type="diagramClass:Method" name="avance"/>
    </classType>
  </greetings>
</diagramClass:Model>
